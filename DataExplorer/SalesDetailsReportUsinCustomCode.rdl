    <DataSet Name="SalesDetailsEmbedDataset">
      <Query>
        <DataSourceName>DbSharedDataSrc</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Region">
            <Value>=Parameters!Region.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT SalePersonFName AS FirstName, SalePersonLName AS LastName, ProductName, ItemsSold, SoldPrice, CONVERT(varchar(25), SoldDate, 106) AS SoldDate, Region
FROM   TotalSale
WHERE (Region IN (@Region))
ORDER BY FirstName</CommandText>
